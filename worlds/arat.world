        <stopButtonStyle>background-color: #D85C48; font: bold 30px;; color: #FFFFFF;</stopButtonStyle>



        <task_group name="Grasp">
          <task id="grasp_1" name="Block 10cm">
            <icon>file://media/materials/textures/grasp_1.jpg</icon>
            <instructions>
              Pick up the 10 cm block and place it on top of the box.
            </instructions>
          </task>

          <task id="grasp_2" name="Block 2.5cm">
            <icon>file://media/materials/textures/grasp_2.jpg</icon>
            <instructions>
              Pick up the 2.5 cm block and place it on top of the box.
            </instructions>
          </task>

          <task id="grasp_3" name="Block 5cm">
            <icon>file://media/materials/textures/grasp_3.jpg</icon>
            <instructions>
              Pick up the 5 cm block and place it on top of the box.
            </instructions>
          </task>

          <task id="grasp_4" name="Block 7.5cm">
            <icon>file://media/materials/textures/grasp_4.jpg</icon>
            <instructions>
              Pick up the 7.5 cm block and place it on top of the box.
            </instructions>

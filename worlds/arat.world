        </task_group>

        <task_group name="Grip">
          <task id="grip_1" name="Cups">
            <icon>file://media/materials/textures/grip_1.jpg</icon>
            <instructions>
              Pour the contents of the filled cup into the empty cup.
            </instructions>
          </task>

          <task id="grip_2" name="Tube 2.25cm">
            <icon>file://media/materials/textures/grip_2.jpg</icon>
            <instructions>
              Pick up the 2.25cm diameter tube and place it on the wooden peg.
            </instructions>
          </task>

          <task id="grip_3" name="Tube 1cm">
            <icon>file://media/materials/textures/grip_3.jpg</icon>
            <instructions>
              Pick up the 1cm diameter tube and place it on the metal peg.
            </instructions>
          </task>

          <task id="grip_4" name="Washer">
            <icon>file://media/materials/textures/grip_4.jpg</icon>
            <instructions>
              Pick up the washer and place it on the metal peg.
            </instructions>

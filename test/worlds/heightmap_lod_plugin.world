<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="default">
    <include><uri>model://camera</uri></include>

    <model name="heightmap">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <heightmap>
              <uri>file://media/materials/textures/heightmap_bowl.png</uri>
              <size>129 129 10</size>
              <pos>0 0 0</pos>
            </heightmap>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <heightmap>
              <uri>file://media/materials/textures/heightmap_bowl.png</uri>
              <size>129 129 10</size>
              <pos>0 0 0</pos>
            </heightmap>
          </geometry>
          <plugin name="lod" filename="libHeightmapLODPlugin.so">
            <lod>5</lod>
            <skirt_length>0.5</skirt_length>
          </plugin>
        </visual>
      </link>
    </model>
  </world>
</sdf>
